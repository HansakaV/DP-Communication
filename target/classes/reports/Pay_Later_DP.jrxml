	<queryString language="SQL">
		<![CDATA[WITH CustomerTotal AS (
    SELECT 
        customer_name,
        SUM(updated_total) AS total_sum
    FROM 
        orders
    WHERE 
        payment = 'Pay Later'
        AND updated_total >= 0
    GROUP BY 
        customer_name
)
SELECT 
    o.date, 
    o.order_id,  
    o.customer_name,
    od.item_name, 
    od.qty, 
    od.unit_price,
    od.total,
    o.payment,
    o.updated_total,
    ct.total_sum
FROM  
    orders o 
JOIN 
    order_detail od 
ON 
    o.order_id = od.order_id 
JOIN 
    CustomerTotal ct
ON 
    o.customer_name = ct.customer_name
WHERE 
    o.payment = 'Pay Later'
    AND o.updated_total >= 0
    AND o.customer_name = $P{customerName}]]>
	</queryString>
