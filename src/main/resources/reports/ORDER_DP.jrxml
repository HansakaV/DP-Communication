	<queryString language="SQL">
		<![CDATA[SELECT 
    o.date, 
    o.order_id, 
    o.customer_name,
    od.item_name,
    od.unit_price,
    od.qty,
    od.total,
    SUM(od.total) OVER (PARTITION BY o.order_id) AS total_sum
FROM 
    orders o 
JOIN 
    order_detail od 
ON 
    o.order_id = od.order_id
    WHERE o.order_id =  $P{Order}]]>
	</queryString>
